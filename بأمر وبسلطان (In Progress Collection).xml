  <lyrics>
    <verse name="v1a">
      <lines>أنت الملك يا يسوع</lines>
    </verse>
    <verse name="v1b">
      <lines>بأمر وبسلطان إبليس يخرج بجنوده</lines>
    </verse>
    <verse name="v1c">
      <lines>وبقول لعدو الخير سيب ارضي وبلادي</lines>
    </verse>
    <verse name="v1d">
      <lines>بأمر بالتحرير</lines>
    </verse>
    <verse name="v1e">
      <lines>لأصحابي وكل جيراني</lines>
    </verse>
    <verse name="v1f">
      <lines>وبقول جنود الشر</lines>
    </verse>
    <verse name="v1g">
      <lines>يلا اهربي من قدامي</lines>
    </verse>
  </lyrics>
</song>
