<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-11-21T08:40:03">
  <properties>
    <titles>
      <title>ارفع اسم الفادى</title>
    </titles>
    <authors>
      <author type="words">غير معروف</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines> ارفع اسم الفادى <br/>ملك حياتى يسوع</lines>
    </verse>
    <verse name="v2">
      <lines>مليك المجد وسيد الشعوب<br/>رئيس خلاصى</lines>
    </verse>
    <verse name="v3">
      <lines>مليك المجد وسيد الشعوب<br/>ملك حياتى للأبد</lines>
    </verse>
  </lyrics>
</song>
