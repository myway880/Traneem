  <lyrics>
    <verse name="v1a">
      <lines>إسم الرب إلهنا<br/>حصن لنا وقوة</lines>
    </verse>
    <verse name="v1b">
      <lines>نفرح به<br/>ونرفع رايات النصرة</lines>
    </verse>
    <verse name="v1c">
      <lines>إسم الرب إلهنا<br/>حصن لنا وقوة</lines>
    </verse>
    <verse name="v1d">
      <lines>نفرح به<br/>ونرفع الرايات</lines>
    </verse>
    <verse name="v1e">
      <lines>نسبح اسمه<br/>ونهتف</lines>
    </verse>
    <verse name="v1f">
      <lines>يقوى الرب على كل أعدائنا</lines>
    </verse>
    <verse name="v1g">
      <lines>وبالأبواق نعلن<br/>نصرتنا</lines>
    </verse>
    <verse name="v1h">
      <lines>مليكنا يرفع رؤوسنا</lines>
    </verse>
  </lyrics>
