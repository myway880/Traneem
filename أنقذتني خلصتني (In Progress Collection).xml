  <lyrics>
    <verse name="v1a">
      <lines>أنقذتني خلصتني بدمك اشتريتني</lines>
    </verse>
    <verse name="v1b">
      <lines>سأرقص أمامك لأنك أبهجتني</lines>
    </verse>
    <verse name="v1c">
      <lines>أقمتني شفيتني بروحك أحييتني</lines>
    </verse>
    <verse name="v1d">
      <lines>للأبد أهتف لمن أحبني</lines>
    </verse>
    <verse name="v1e">
      <lines>أبقى ملكا لك للأبد</lines>
    </verse>
    <verse name="v1f">
      <lines>أشهد عن حبك للأبد</lines>
    </verse>
  </lyrics>
</song>
