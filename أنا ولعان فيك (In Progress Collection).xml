  <lyrics>
    <verse name="v1a">
      <lines>إنت حبيبي يا يسوع</lines>
    </verse>
    <verse name="v1b">
      <lines>إنت نصيبي يا يسوع</lines>
    </verse>
    <verse name="v1c">
      <lines>أنا ولعان فيك</lines>
    </verse>
    <verse name="v1d">
      <lines>أنا فرحان فيك</lines>
    </verse>
    <verse name="v1e">
      <lines>إنت حبيبي يا يسوع</lines>
    </verse>
    <verse name="v1f">
      <lines>ومهما يقولوا ويقولوا الناس</lines>
    </verse>
    <verse name="v1g">
      <lines>راح ضل أحبك من دون مقياس</lines>
    </verse>
    <verse name="v1h">
      <lines>قدام العالم شوف حالي فيك</lines>
    </verse>
    <verse name="v1i">
      <lines>بغنيك حبي بقلبي شاريك</lines>
    </verse>
  </lyrics>
