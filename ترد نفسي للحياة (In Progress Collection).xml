  <lyrics>
    <verse name="v1a">
      <lines>ترد نفسي<br/>ترد نفسي<br/>ترد نفسي<br/>للحياة</lines>
    </verse>
    <verse name="v1b">
      <lines>حملت آلامي<br/>دعوتني بإسم جديد</lines>
    </verse>
    <verse name="v1c">
      <lines>أخذت كل خزيي<br/>بدلته بأفراح</lines>
    </verse>
    <verse name="v1d">
      <lines>تاخذ نوحي تحوله لرقص</lines>
    </verse>
    <verse name="v1e">
      <lines>تاخذ بكائي تبدله بضحكا</lines>
    </verse>
    <verse name="v1f">
      <lines>تاخذ نوحي تحوله لرقص</lines>
    </verse>
    <verse name="v1g">
      <lines>ترفع حزني وتعطيني أفراح</lines>
    </verse>
    <verse name="v1h">
      <lines>تعطيني أفراح<br/>تعطيني أفراح<br/>تعطيني أفراح<br/>وحياة</lines>
    </verse>
    <verse name="v1i">
      <lines>ترد نفسي<br/>ترد نفسي<br/>ترد نفسي<br/>للحياة</lines>
    </verse>
    <verse name="v1j">
      <lines>هللويا هللويا<br/>صار الكل جديدا</lines>
    </verse>
