  <lyrics>
    <verse name="v1a">
      <lines>طهرني يارب من كل إثمي</lines>
    </verse>
    <verse name="v1b">
      <lines>قدسني يارب غيرن قلبي</lines>
    </verse>
    <verse name="v1c">
      <lines>أنا الضعيف ألتجي إليك</lines>
    </verse>
    <verse name="v1d">
      <lines>هذي حياتي احفظها لديك</lines>
    </verse>
    <verse name="v1e">
      <lines>أنت نبع القوة أنت سندي</lines>
    </verse>
    <verse name="v1f">
      <lines>بك سوف أحتمي بك أهتدي</lines>
    </verse>
  </lyrics>
</song>
