  <lyrics>
    <verse name="c1">
      <lines>في مزيكا في قلبي<br/>لربي وحبيبي يسوع</lines>
    </verse>
    <verse name="v1">
      <lines> صابعك فوق</lines>
    </verse>
    <verse name="v2">
      <lines> صابعك فوق<br/>كوعك برة</lines>
    </verse>
    <verse name="v3">
      <lines> صابعك فوق<br/>كوعك برة<br/>اتني ركبك</lines>
    </verse>
    <verse name="v4">
      <lines> صابعك فوق<br/>كوعك برة<br/>لف معايا</lines>
    </verse>
    <verse name="v5">
      <lines> صابعك فوق<br/>صقف معايا</lines>
    </verse>
    <verse name="v6">
      <lines> صابعك فوق<br/>نط معايا</lines>
    </verse>
  </lyrics>
</song>
