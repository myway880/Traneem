  <lyrics>
    <verse name="v1a">
      <lines>أبيض<br/>شبه الحمام</lines>
    </verse>
    <verse name="v1b">
      <lines>لامع<br/>زي النور تمام</lines>
    </verse>
    <verse name="v1c">
      <lines>اسمع<br/>دا صوت الملاك</lines>
    </verse>
    <verse name="v1d">
      <lines>هاتف<br/>رب المجد لك</lines>
    </verse>
    <verse name="v1e">
      <lines>صارخ<br/>رب الصاباؤوت</lines>
    </verse>
    <verse name="v1f">
      <lines>مجدك<br/>ملا الملكوت</lines>
    </verse>
    <verse name="v1g">
      <lines>خادم<br/>اله الانام</lines>
    </verse>
    <verse name="v1h">
      <lines>هادي<br/>يحب السلام</lines>
    </verse>
    <verse name="v1i">
      <lines>ساهر<br/>في كل اوان</lines>
    </verse>
    <verse name="v1j">
      <lines>حارس<br/>لكل انسان</lines>
    </verse>
  </lyrics>
</song>
