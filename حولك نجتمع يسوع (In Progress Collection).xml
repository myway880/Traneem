  <lyrics>
    <verse name="v1a">
      <lines>حولك نجتمع يسوع نجدد العهود</lines>
    </verse>
    <verse name="v1b">
      <lines>نهلل نرنم معك يحلو الوجود</lines>
    </verse>
    <verse name="v1c">
      <lines>نرفع أيادينا نطلب أمانينا</lines>
    </verse>
    <verse name="v1d">
      <lines>ندعو فادينا يسوع البار</lines>
    </verse>
    <verse name="v1e">
      <lines>روحك معزي يدك تنجي</lines>
    </verse>
    <verse name="v1f">
      <lines>أنت ملاذي ياربي وسط الأخطار</lines>
    </verse>
  </lyrics>
</song>
