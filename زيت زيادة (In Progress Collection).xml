    <verse name="v1a">
      <lines>زيت زيادة فيض علينا</lines>
    </verse>
    <verse name="v1b">
      <lines>زيت زيادة املأ إيدينا</lines>
    </verse>
    <verse name="v1c">
      <lines>زيت زيادة إمسحنا يا الله</lines>
    </verse>
