  <lyrics>
    <verse name="v1a">
      <lines>سجودا ندخل حجالك</lines>
    </verse>
    <verse name="v1b">
      <lines>لعمق قدس الاقداس</lines>
    </verse>
    <verse name="v1c">
      <lines>نسجد هناك</lines>
    </verse>
    <verse name="v1d">
      <lines>فنري مجدك السماوى</lines>
    </verse>
    <verse name="v1e">
      <lines>يملأ كل الحياة</lines>
    </verse>
    <verse name="v1f">
      <lines>كل الحياة</lines>
    </verse>
    <verse name="v1g">
      <lines>هللويا مجدا لاسمك</lines>
    </verse>
    <verse name="v1h">
      <lines>رب المجد ليس مثلك</lines>
    </verse>
    <verse name="v1i">
      <lines>إلهى يشق البحار وينقل كل الجبال</lines>
    </verse>
    <verse name="v1j">
      <lines>كل الجبال</lines>
    </verse>
    <verse name="v1k">
      <lines>بروحه نبصر الايات</lines>
    </verse>
    <verse name="v1l">
      <lines>فليس أمر محال فوق الخيال</lines>
    </verse>
  </lyrics>
</song>
