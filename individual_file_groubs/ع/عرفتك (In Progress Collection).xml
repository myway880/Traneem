  <lyrics>
    <verse name="v1a">
      <lines>عرفتك يا سيدي</lines>
    </verse>
    <verse name="v1b">
      <lines>عرفتك حين رأيتك وسط اللهيب</lines>
    </verse>
    <verse name="v1c">
      <lines>وحين تصاعد عطر الفداء بخورا نقيا زكيا</lines>
    </verse>
    <verse name="v1d">
      <lines>بنار الصليب</lines>
    </verse>
    <verse name="v1e">
      <lines>عرفتك</lines>
    </verse>
    <verse name="v1f">
      <lines>حين رأيتك ترنو إلى صالبيك</lines>
    </verse>
    <verse name="v1g">
      <lines>وتحنو عليهم بحب عجيب</lines>
    </verse>
    <verse name="v1h">
      <lines>وقد ذقت في أرضنا جفوة</lines>
    </verse>
    <verse name="v1i">
      <lines>وانت الرقيق</lines>
    </verse>
    <verse name="v1j">
      <lines>وأنت الوديع</lines>
    </verse>
    <verse name="v1k">
      <lines>وأنت الحبيب</lines>
    </verse>
  </lyrics>
</song>
