  <lyrics>
    <verse name="v1a">
      <lines>افتح عيني يا ربي</lines>
    </verse>
    <verse name="v1b">
      <lines>افتح عيني عليك</lines>
    </verse>
    <verse name="v1c">
      <lines>عايز أشوفك</lines>
    </verse>
    <verse name="v1d">
      <lines>أشوفك عالي فوق الكل</lines>
    </verse>
    <verse name="v1e">
      <lines>حبك يملا قلبي وكياني</lines>
    </verse>
    <verse name="v1f">
      <lines>تملك علي بالحب</lines>
    </verse>
    <verse name="v1g">
      <lines>وأعيش لك كل يوم في حياتي</lines>
    </verse>
    <verse name="v1h">
      <lines>باسبحك يا إلهي عايز أشوفك</lines>
    </verse>
  </lyrics>
</song>
