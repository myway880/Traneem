  <lyrics>
    <verse name="v1a">
      <lines>ألوف ألوف تخدمه</lines>
    </verse>
    <verse name="v1b">
      <lines>وربوات ربوات قدامه</lines>
    </verse>
    <verse name="v1c">
      <lines>هييجي مع سحب السماء</lines>
    </verse>
    <verse name="v1d">
      <lines>إبن الإنسان مع خدامه</lines>
    </verse>
    <verse name="v1e">
      <lines>أعطي سلطانا ومجد لتعبده كل الشعوب</lines>
    </verse>
    <verse name="v1f">
      <lines>كل الامم والالسنه</lines>
    </verse>
    <verse name="v1g">
      <lines>ملكوته على الكل بيسود</lines>
    </verse>
    <verse name="v1h">
      <lines>سلطانه سلطان أبدي ملكوته</lines>
    </verse>
    <verse name="v1i">
      <lines>ماله حدود ابن الإنسان</lines>
    </verse>
    <verse name="v1j">
      <lines>وجهه</lines>
    </verse>
    <verse name="v1k">
      <lines>كمنظر البرق عيناه كمصباحي نار</lines>
    </verse>
    <verse name="v1l">
      <lines>كلامه كصوت الجمهور بدمه</lines>
    </verse>
    <verse name="v1m">
      <lines>أصبحنا أبرار</lines>
    </verse>
    <verse name="v1n">
      <lines>لمسته لمسه قوية بيها</lines>
    </verse>
    <verse name="v1o">
      <lines>يغير الاشرار إبن الإنسان</lines>
    </verse>
    <verse name="v1p">
      <lines>سلطانه</lines>
    </verse>
    <verse name="v1q">
      <lines>إلى المنتهى ليه الأرض والسماوات</lines>
    </verse>
    <verse name="v1r">
      <lines>حبني الى المنتهى بصليبه</lines>
    </verse>
    <verse name="v1s">
      <lines>شال الألمات</lines>
    </verse>
    <verse name="v1t">
      <lines>ليه وحده كل الإكرام</lines>
    </verse>
    <verse name="v1u">
      <lines>الساكن في التسبيحات ابن الانسان</lines>
    </verse>
  </lyrics>
</song>
