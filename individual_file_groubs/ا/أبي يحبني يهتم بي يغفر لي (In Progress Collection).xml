  <lyrics>
    <verse name="v1a">
      <lines>أبي يحبني يهتم بي يغفر لي</lines>
    </verse>
    <verse name="v1b">
      <lines>يترأف علي أبي</lines>
    </verse>
    <verse name="v1c">
      <lines>يهبني الحياة يعلم أموري</lines>
    </verse>
    <verse name="v1d">
      <lines>من الشر يحفظني أبي</lines>
    </verse>
    <verse name="v1e">
      <lines>يقبلني يشبعني يطلبني</lines>
    </verse>
    <verse name="v1f">
      <lines>يستمع إلي</lines>
    </verse>
    <verse name="v1g">
      <lines>يشفيني يعوضني يتحنن علي</lines>
    </verse>
    <verse name="v1h">
      <lines>أبي</lines>
    </verse>
