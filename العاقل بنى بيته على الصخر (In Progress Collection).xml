  <lyrics>
    <verse name="v1a">
      <lines>العاقل بنى بيته عالصخر</lines>
    </verse>
    <verse name="v1b">
      <lines>والأمطار انسكبت</lines>
    </verse>
    <verse name="v1c">
      <lines>الامطار انسكبت والينابيع انفجرت والبيت</lines>
    </verse>
    <verse name="v1d">
      <lines>على الصخر ثبت</lines>
    </verse>
    <verse name="v1e">
      <lines>الجاهل بنى بيته عالرمل والأمطار انسكبت</lines>
    </verse>
    <verse name="v1f">
      <lines>الامطار انسكبت والينابيع انفجرت والبيت</lines>
    </verse>
    <verse name="v1g">
      <lines>على الرمل سقط</lines>
    </verse>
    <verse name="v1h">
      <lines>فلنبني حياتنا على الرب يسوعفلتأتي</lines>
    </verse>
    <verse name="v1i">
      <lines>البركات</lines>
    </verse>
    <verse name="v1j">
      <lines>تأتي البركات حين ترفع الصلوات وحياتنا</lines>
    </verse>
    <verse name="v1k">
      <lines>بالرب تثبت</lines>
    </verse>
  </lyrics>
</song>
