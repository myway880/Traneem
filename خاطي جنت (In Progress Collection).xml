  <lyrics>
    <verse name="v1a">
      <lines>خاطي جنت وية اليخطئون</lines>
    </verse>
    <verse name="v1b">
      <lines>ودمعي نزل من وسط العيون</lines>
    </verse>
    <verse name="v1c">
      <lines>قالوا ترة هذولة ميخلصونوبشرهم رح</lines>
    </verse>
    <verse name="v1d">
      <lines>يظلون لكن</lines>
    </verse>
    <verse name="v1e">
      <lines>حبيبي القمر خلصني وخلص البشر</lines>
    </verse>
    <verse name="v1f">
      <lines>بجرعه كاس المر وبقيامته من</lines>
    </verse>
    <verse name="v1g">
      <lines>القبر نسهر</lines>
    </verse>
    <verse name="v1h">
      <lines>ونصلي ليالي ننتظر جيتك يا غالي</lines>
    </verse>
    <verse name="v1i">
      <lines>بفرحة وببهجة وأغانيحنروح وياك</lines>
    </verse>
    <verse name="v1j">
      <lines>للأعالي يسوع حبيبي</lines>
    </verse>
    <verse name="v1k">
      <lines>القمر خلصني وخلص البشر</lines>
    </verse>
    <verse name="v1l">
      <lines>بجرعه كاس المر وبقيامته من</lines>
    </verse>
    <verse name="v1m">
      <lines>القبر</lines>
    </verse>
